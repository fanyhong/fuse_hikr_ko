<Page ux:Class="HomePage">
    <!--router 이름의 디펜던시 Router 인스턴스를 생성 (주입 받아 동작함)-->
    <Router ux:Dependency="router" />
    <JavaScript File="HomePage.js" />
    <ScrollView>
        <StackPanel>
            <Each Items="{hikes}">
                <Button Text="{name}" Clicked="{goHike}" />
            </Each>
        </StackPanel>
    </ScrollView>
</Page>